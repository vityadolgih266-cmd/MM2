-- üîî –£–í–ï–î–û–ú–õ–ï–ù–ò–ï –û –ó–ê–ü–£–°–ö–ï
Rayfield:Notify({
   Title = "üî• VOLT X HUB –ê–ö–¢–ò–í–ò–†–û–í–ê–ù",
   Content = "–≠–ª–∏—Ç–Ω—ã–π —á–∏—Ç-–º–æ–¥ —É—Å–ø–µ—à–Ω–æ –∑–∞–ø—É—â–µ–Ω!\n–≠–∫—Å–∫–ª—é–∑–∏–≤–Ω—ã–µ —Ñ—É–Ω–∫—Ü–∏–∏ –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω—ã",
   Duration = 6,
   Image = 4483362458
})

-- üîÑ –û–°–ù–û–í–ù–û–ô –¶–ò–ö–õ
RunService.Heartbeat:Connect(function()
   -- ESP
   UpdateVoltESP()
   UpdateChams()
   
   -- –ö–æ–º–±–∞—Ç
   VoltAimbot()
   AutoKnife()
   
   -- –î–≤–∏–∂–µ–Ω–∏–µ
   VoltFly()
   VoltSpeed()
   
   -- Noclip
   if Volt.Movement.Noclip and LP.Character then
       for _, part in pairs(LP.Character:GetDescendants()) do
           if part:IsA("BasePart") then
               part.CanCollide = false
           end
       end
   end
   
   -- –ê–≤—Ç–æ–º–∞—Ç–∏–∑–∞—Ü–∏—è
   if Volt.Automation.AutoFarm then
       -- Auto Farm –ª–æ–≥–∏–∫–∞
   end
   
   if Volt.Automation.AutoCandy then
       AutoCandy()
   end
   
   -- –£–∫–ª–æ–Ω–µ–Ω–∏–µ –æ—Ç —É–±–∏–π—Ü—ã
   if Volt.Automation.AvoidMurderer then
       for _, player in pairs(Players:GetPlayers()) do
           if player ~= LP and GetRole(player) == "Murderer" and player.Character then
               local dist = (LP.Character.HumanoidRootPart.Position - player.Character.HumanoidRootPart.Position).Magnitude
               if dist < 20 then
                   local dir = (LP.Character.HumanoidRootPart.Position - player.Character.HumanoidRootPart.Position).Unit
                   LP.Character.HumanoidRootPart.CFrame = LP.Character.HumanoidRootPart.CFrame + dir * 25
               end
           end
       end
   end
   
   -- Infinite Jump
   if Volt.Movement.InfJump then
       if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
           LP.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Jumping")
       end
   end
end)

-- üéØ –ò–ù–ò–¶–ò–ê–õ–ò–ó–ê–¶–ò–Ø
for _, player in pairs(Players:GetPlayers()) do
   if player ~= LP then 
       CreateVoltESP(player)
       if Volt.ESP.Chams then
           CreateChams(player)
       end
   end
end

Players.PlayerAdded:Connect(function(player)
   player.CharacterAdded:Connect(function()
       wait(1)
       CreateVoltESP(player)
       if Volt.ESP.Chams then
           CreateChams(player)
       end
   end)
end)

Players.PlayerRemoving:Connect(function(player)
   if ESP[player] then
       for _, drawing in pairs(ESP[player]) do
           if type(drawing) == "table" then
               for _, line in pairs(drawing) do
                   if line then line:Remove() end
               end
           elseif drawing then
               drawing:Remove()
           end
       end
       ESP[player] = nil
   end
   
   if Chams[player] then
       for _, highlight in pairs(Chams[player]) do
           highlight:Destroy()
       end
       Chams[player] = nil
   end
end)

-- üõ°Ô∏è –ó–ê–ü–£–°–ö –°–ò–°–¢–ï–ú
VoltAntiAFK()

print("üî• Volt X Hub —É—Å–ø–µ—à–Ω–æ –∑–∞–≥—Ä—É–∂–µ–Ω!")
print("‚ú® –í–µ—Ä—Å–∏—è 2.0 | –°–æ–∑–¥–∞–Ω–æ AI Assistant")
print("üéÆ –≠–∫—Å–∫–ª—é–∑–∏–≤–Ω—ã–µ —Ñ—É–Ω–∫—Ü–∏–∏ –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω—ã!")
