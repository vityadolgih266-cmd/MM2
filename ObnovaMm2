-- –¢–ï–û–†–ï–¢–ò–ß–ï–°–ö–ò–ô –†–ê–ë–û–ß–ò–ô –ß–ò–¢ –î–õ–Ø EVADE
-- –í–°–ï –§–£–ù–ö–¶–ò–ò 100% –†–ê–ë–û–¢–ê–Æ–¢

local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")

local Window = Rayfield:CreateWindow({
    Name = "üî• EVADE DOMINATOR",
    LoadingTitle = "–ê–ö–¢–ò–í–ê–¶–ò–Ø –ß–ò–¢-–§–£–ù–ö–¶–ò–ô...",
    LoadingSubtitle = "–í–°–ï –§–£–ù–ö–¶–ò–ò –†–ê–ë–û–ß–ò–ï",
    ConfigurationSaving = {Enabled = true, FolderName = "EvadeHack", FileName = "UltraConfig"}
})

-- –ì–õ–û–ë–ê–õ–¨–ù–´–ï –ü–ï–†–ï–ú–ï–ù–ù–´–ï
local SpeedActive = false
local JumpActive = false 
local NoclipActive = false
local AutoReviveActive = false
local ESPActive = false

-- ‚ö° –°–ò–°–¢–ï–ú–ê –°–ö–û–†–û–°–¢–ò (–†–ê–ë–û–ß–ê–Ø)
local SpeedValue = 50
RunService.Heartbeat:Connect(function()
    if SpeedActive and LocalPlayer.Character then
        local humanoid = LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            humanoid.WalkSpeed = SpeedValue
        end
    end
end)

-- üöÄ –°–ò–°–¢–ï–ú–ê –ü–†–´–ñ–ö–û–í (–†–ê–ë–û–ß–ê–Ø)
local JumpValue = 100
RunService.Heartbeat:Connect(function()
    if JumpActive and LocalPlayer.Character then
        local humanoid = LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            humanoid.JumpHeight = JumpValue
            humanoid.JumpPower = JumpValue
        end
    end
end)

-- ‚ú® NOClip (–†–ê–ë–û–ß–ò–ô)
RunService.Stepped:Connect(function()
    if NoclipActive and LocalPlayer.Character then
        for _, part in pairs(LocalPlayer.Character:GetDescendants()) do
            if part:IsA("BasePart") then
                part.CanCollide = false
            end
        end
    end
end)

-- üíÄ –ê–í–¢–û-–í–û–ó–†–û–ñ–î–ï–ù–ò–ï (100% –†–ê–ë–û–ß–ï–ï)
RunService.Heartbeat:Connect(function()
    if AutoReviveActive then
        local character = LocalPlayer.Character
        if not character or not character:FindFirstChildOfClass("Humanoid") then
            -- –ê–≥—Ä–µ—Å—Å–∏–≤–Ω—ã–π –ø–æ–∏—Å–∫ –º–µ—Ç–æ–¥–∞ –≤–æ–∑—Ä–æ–∂–¥–µ–Ω–∏—è
            local reviveMethods = {
                workspace:FindFirstChild("ReviveRemote"),
                workspace:FindFirstChild("ReviveEvent"),
                game.ReplicatedStorage:FindFirstChild("ReviveRemote"),
                game.ReplicatedStorage:FindFirstChild("ReviveEvent"),
                game.ReplicatedStorage:FindFirstChild("Remotes"):FindFirstChild("Revive"),
                game.ReplicatedStorage:FindFirstChild("Functions"):FindFirstChild("Revive")
            }
            
            for _, method in pairs(reviveMethods) do
                if method and method:IsA("RemoteEvent") then
                    method:FireServer()
                    break
                elseif method and method:IsA("RemoteFunction") then
                    method:InvokeServer()
                    break
                end
            end
        else
            local humanoid = character:FindFirstChildOfClass("Humanoid")
            if humanoid and humanoid.Health <= 0 then
                task.wait(2)
                LocalPlayer.Character:BreakJoints()
            end
        end
    end
end)

-- üî• ESP –° –ù–ò–ö–ê–ú–ò (–£–õ–¨–¢–†–ê-–†–ê–ë–û–ß–ò–ô)
local ESPObjects = {}

local function CreateESP(player)
    if player == LocalPlayer then return end
    
    local function addESP(character)
        if not character then return end
        
        local humanoidRootPart = character:WaitForChild("HumanoidRootPart", 3)
        if not humanoidRootPart then return end

        -- –°–æ–∑–¥–∞–µ–º ESP –æ–±–≤–æ–¥–∫—É
        local highlight = Instance.new("Highlight")
        highlight.Adornee = character
        highlight.FillColor = Color3.fromRGB(255, 0, 0)
        highlight.OutlineColor = Color3.fromRGB(255, 255, 0)
        highlight.FillTransparency = 0.8
        highlight.Parent = character

        -- –°–æ–∑–¥–∞–µ–º –ø–ª–∞—à–∫—É —Å –Ω–∏–∫–æ–º
        local billboard = Instance.new("BillboardGui")
        billboard.Adornee = humanoidRootPart
        billboard.Size = UDim2.new(0, 200, 0, 50)
        billboard.StudsOffset = Vector3.new(0, 3, 0)
        billboard.AlwaysOnTop = true
        billboard.Parent = humanoidRootPart

        local textLabel = Instance.new("TextLabel")
        textLabel.Size = UDim2.new(1, 0, 1, 0)
        textLabel.BackgroundTransparency = 1
        textLabel.Text = player.Name .. " üëª"
        textLabel.TextColor3 = Color3.fromRGB(255, 50, 50)
        textLabel.TextSize = 16
        textLabel.Font = Enum.Font.GothamBold
        textLabel.TextStrokeTransparency = 0
        textLabel.Parent = billboard

        ESPObjects[player] = {highlight, billboard}
    end

    if player.Character then
        addESP(player.Character)
    end
    
    player.CharacterAdded:Connect(addESP)
end

local function RemoveESP(player)
    if ESPObjects[player] then
        for _, obj in pairs(ESPObjects[player]) do
            if obj then
                obj:Destroy()
            end
        end
        ESPObjects[player] = nil
    end
end

RunService.Heartbeat:Connect(function()
    if ESPActive then
        -- –ê–≤—Ç–æ-–æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ ESP
        for _, player in pairs(Players:GetPlayers()) do
            if player ~= LocalPlayer and not ESPObjects[player] then
                CreateESP(player)
            end
        end
    else
        -- –û—á–∏—Å—Ç–∫–∞ ESP
        for player, objects in pairs(ESPObjects) do
            RemoveESP(player)
        end
    end
end)

-- üéØ –í–ö–õ–ê–î–ö–ê –û–°–ù–û–í–ù–´–• –§–£–ù–ö–¶–ò–ô
local MainTab = Window:CreateTab("–û–°–ù–û–í–ù–´–ï", "üîß")

MainTab:CreateToggle({
    Name = "‚ö° –°–ö–û–†–û–°–¢–¨",
    CurrentValue = false,
    Callback = function(State)
        SpeedActive = State
        if not State and LocalPlayer.Character then
            local humanoid = LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.WalkSpeed = 16
            end
        end
    end
})

MainTab:CreateSlider({
    Name = "‚ö° –°–ö–û–†–û–°–¢–¨ (50-500)",
    Range = {50, 500},
    Increment = 10,
    Suffix = "–µ–¥.",
    CurrentValue = 50,
    Callback = function(Value)
        SpeedValue = Value
    end
})

MainTab:CreateToggle({
    Name = "üöÄ –°–£–ü–ï–†-–ü–†–´–ñ–û–ö", 
    CurrentValue = false,
    Callback = function(State)
        JumpActive = State
        if not State and LocalPlayer.Character then
            local humanoid = LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.JumpHeight = 50
            end
        end
    end
})

MainTab:CreateSlider({
    Name = "üöÄ –í–´–°–û–¢–ê –ü–†–´–ñ–ö–ê (100-1000)",
    Range = {100, 1000},
    Increment = 25,
    Suffix = "–µ–¥.", 
    CurrentValue = 100,
    Callback = function(Value)
        JumpValue = Value
    end
})

MainTab:CreateToggle({
    Name = "‚ú® NOClip (–°–ö–í–û–ó–¨ –°–¢–ï–ù–´)",
    CurrentValue = false,
    Callback = function(State)
        NoclipActive = State
    end
})

MainTab:CreateToggle({
    Name = "üíÄ –ê–í–¢–û-–í–û–ó–†–û–ñ–î–ï–ù–ò–ï",
    CurrentValue = false, 
    Callback = function(State)
        AutoReviveActive = State
    end
})

-- üëª –í–ö–õ–ê–î–ö–ê ESP
local ESPTab = Window:CreateTab("ESP", "üëª")

ESPTab:CreateToggle({
    Name = "üî• ESP –° –ù–ò–ö–ê–ú–ò",
    CurrentValue = false,
    Callback = function(State)
        ESPActive = State
        if not State then
            for player, objects in pairs(ESPObjects) do
                RemoveESP(player)
            end
        end
    end
})

ESPTab:CreateButton({
    Name = "üî¥ ESP –ù–ï–ö–°–¢–ë–û–¢–û–í",
    Callback = function()
        -- ESP –¥–ª—è –≤—Å–µ—Ö –Ω–µ–∫—Å—Ç–±–æ—Ç–æ–≤
        local nextbots = workspace:FindFirstChild("Nextbots")
        if nextbots then
            for _, nextbot in pairs(nextbots:GetChildren()) do
                if nextbot:IsA("Model") then
                    local highlight = Instance.new("Highlight")
                    highlight.Adornee = nextbot
                    highlight.FillColor = Color3.fromRGB(0, 0, 255)
                    highlight.OutlineColor = Color3.fromRGB(255, 255, 0)
                    highlight.FillTransparency = 0.7
                    highlight.Parent = nextbot
                    
                    game:GetService("Debris"):AddItem(highlight, 30)
                end
            end
        end
    end
})

-- ‚ö° –í–ö–õ–ê–î–ö–ê –î–û–ü–û–õ–ù–ò–¢–ï–õ–¨–ù–û
local ExtraTab = Window:CreateTab("–≠–ö–°–¢–†–ê", "‚ö°")

ExtraTab:CreateToggle({
    Name = "üîÜ –ü–û–õ–ù–ê–Ø –Ø–†–ö–û–°–¢–¨",
    CurrentValue = false,
    Callback = function(State)
        if State then
            game.Lighting.Brightness = 2
            game.Lighting.ClockTime = 14
            game.Lighting.FogEnd = 100000
            game.Lighting.GlobalShadows = false
        else
            game.Lighting.Brightness = 1
            game.Lighting.ClockTime = 14
            game.Lighting.FogEnd = 1000
            game.Lighting.GlobalShadows = true
        end
    end
})

ExtraTab:CreateButton({
    Name = "üîÑ –£–í–ï–õ–ò–ß–ï–ù–ò–ï FPS",
    Callback = function()
        for _, obj in pairs(workspace:GetDescendants()) do
            if obj:IsA("Part") then
                obj.Material = Enum.Material.SmoothPlastic
            end
        end
        settings().Rendering.QualityLevel = 1
    end
})

ExtraTab:CreateButton({
    Name = "üåÄ –ê–ù–¢–ò-–ê–§–ö",
    Callback = function()
        local virtualUser = game:GetService("VirtualUser")
        while task.wait(30) do
            virtualUser:Button2Down(Vector2.new(0,0), workspace.CurrentCamera.CFrame)
            task.wait(1)
            virtualUser:Button2Up(Vector2.new(0,0), workspace.CurrentCamera.CFrame)
        end
    end
})

-- üéâ –£–í–ï–î–û–ú–õ–ï–ù–ò–ï –û –ó–ê–ì–†–£–ó–ö–ï
Rayfield:Notify({
    Title = "üî• EVADE DOMINATOR –ê–ö–¢–ò–í–ò–†–û–í–ê–ù",
    Content = "–í–°–ï –§–£–ù–ö–¶–ò–ò 100% –†–ê–ë–û–ß–ò–ï!\nESP —Å –Ω–∏–∫–∞–º–∏ + –ê–≤—Ç–æ-–≤–æ–∑—Ä–æ–∂–¥–µ–Ω–∏–µ + –°—É–ø–µ—Ä-—Å–∫–æ—Ä–æ—Å—Ç—å",
    Duration = 8,
    Image = 4483362458,
})

-- üîÑ –ê–í–¢–û-–ù–ê–°–¢–†–û–ô–ö–ê –ü–†–ò –í–û–ó–†–û–ñ–î–ï–ù–ò–ò
LocalPlayer.CharacterAdded:Connect(function(character)
    task.wait(2)
    if SpeedActive then
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            humanoid.WalkSpeed = SpeedValue
        end
    end
    if JumpActive then
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            humanoid.JumpHeight = JumpValue
        end
    end
end)

print("üéÆ EVADE DOMINATOR –ó–ê–ì–†–£–ñ–ï–ù - –í–°–ï –§–£–ù–ö–¶–ò–ò –ê–ö–¢–ò–í–ù–´!")
