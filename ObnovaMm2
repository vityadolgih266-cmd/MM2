-- ðŸŽ¯ ÐŸÐ Ð•ÐœÐ˜Ð£Ðœ Ð¡Ð˜Ð¡Ð¢Ð•ÐœÐ ESP
local ESPObjects = {}
local Skeletons = {}

local function CreateSkeleton(player)
    if Skeletons[player] then return end
    
    local character = player.Character
    if not character then return end
    
    Skeletons[player] = {}
    local skeleton = Skeletons[player]
    
    -- Ð¡Ð¾Ð·Ð´Ð°ÐµÐ¼ Ð»Ð¸Ð½Ð¸Ð¸ Ð´Ð»Ñ ÑÐºÐµÐ»ÐµÑ‚Ð°
    local connections = {
        {"Head", "UpperTorso"},
        {"UpperTorso", "LowerTorso"},
        {"LowerTorso", "LeftUpperLeg"},
        {"LowerTorso", "RightUpperLeg"},
        {"LeftUpperLeg", "LeftLowerLeg"},
        {"RightUpperLeg", "RightLowerLeg"},
        {"LeftLowerLeg", "LeftFoot"},
        {"RightLowerLeg", "RightFoot"},
        {"UpperTorso", "LeftUpperArm"},
        {"UpperTorso", "RightUpperArm"},
        {"LeftUpperArm", "LeftLowerArm"},
        {"RightUpperArm", "RightLowerArm"},
        {"LeftLowerArm", "LeftHand"},
        {"RightLowerArm", "RightHand"}
    }
    
    for _, connection in pairs(connections) do
        local part1 = character:FindFirstChild(connection[1])
        local part2 = character:FindFirstChild(connection[2])
        
        if part1 and part2 then
            local line = Drawing.new("Line")
            line.Thickness = 2
            line.Color = RoleColors.Innocent
            line.Visible = false
            table.insert(skeleton, line)
        end
    end
end

local function UpdateSkeleton(player)
    local skeleton = Skeletons[player]
    if not skeleton then return end
    
    local character = player.Character
    if not character then return end
    
    local lines = skeleton
    local connections = {
        {"Head", "UpperTorso"},
        {"UpperTorso", "LowerTorso"},
        {"LowerTorso", "LeftUpperLeg"},
        {"LowerTorso", "RightUpperLeg"},
        {"LeftUpperLeg", "LeftLowerLeg"},
        {"RightUpperLeg", "RightLowerLeg"},
        {"LeftLowerLeg", "LeftFoot"},
        {"RightLowerLeg", "RightFoot"},
        {"UpperTorso", "LeftUpperArm"},
        {"UpperTorso", "RightUpperArm"},
        {"LeftUpperArm", "LeftLowerArm"},
        {"RightUpperArm", "RightLowerArm"},
        {"LeftLowerArm", "LeftHand"},
        {"RightLowerArm", "RightHand"}
    }
    
    for i, connection in pairs(connections) do
        local line = lines[i]
        if line then
            local part1 = character:FindFirstChild(connection[1])
            local part2 = character:FindFirstChild(connection[2])
            
            if part1 and part2 then
                local pos1, visible1 = Camera:WorldToViewportPoint(part1.Position)
                local pos2, visible2 = Camera:WorldToViewportPoint(part2.Position)
                
                if visible1 and visible2 then
                    line.From = Vector2.new(pos1.X, pos1.Y)
                    line.To = Vector2.new(pos2.X, pos2.Y)
                    line.Color = RoleColors.Innocent
                    line.Visible = CheatState.ESP.Skeleton and CheatState.ESP.Enabled
                else
                    line.Visible = false
                end
            else
                line.Visible = false
            end
        end
    end
end

local function CreateESP(player)
    if ESPObjects[player] then return end
    
    ESPObjects[player] = {
        Box = Drawing.new("Square"),
        Name = Drawing.new("Text"),
        Tracer = Drawing.new("Line"),
        HealthBar = Drawing.new("Line"),
        HealthText = Drawing.new("Text"),
        Distance = Drawing.new("Text"),
        Role = Drawing.new("Text"),
        Weapon = Drawing.new("Text")
    }
    
    local esp = ESPObjects[player]
    
    -- ÐÐ°ÑÑ‚Ñ€Ð¾Ð¹ÐºÐ° Box
    esp.Box.Thickness = 2
    esp.Box.Filled = false
    esp.Box.ZIndex = 1
    
    -- ÐÐ°ÑÑ‚Ñ€Ð¾Ð¹ÐºÐ° Name
    esp.Name.Size = 14
    esp.Name.Center = true
    esp.Name.Outline = true
    esp.Name.ZIndex = 2
    
    -- ÐÐ°ÑÑ‚Ñ€Ð¾Ð¹ÐºÐ° Tracer
    esp.Tracer.Thickness = 1
    esp.Tracer.ZIndex = 1
    
    -- ÐÐ°ÑÑ‚Ñ€Ð¾Ð¹ÐºÐ° HealthBar
    esp.HealthBar.Thickness = 3
    esp.HealthBar.ZIndex = 1
    
    -- ÐÐ°ÑÑ‚Ñ€Ð¾Ð¹ÐºÐ° HealthText
    esp.HealthText.Size = 12
    esp.HealthText.Outline = true
    esp.HealthText.ZIndex = 2
    
    -- ÐÐ°ÑÑ‚Ñ€Ð¾Ð¹ÐºÐ° Distance
    esp.Distance.Size = 12
    esp.Distance.Center = true
    esp.Distance.Outline = true
    esp.Distance.ZIndex = 2
    
    -- ÐÐ°ÑÑ‚Ñ€Ð¾Ð¹ÐºÐ° Role
    esp.Role.Size = 12
    esp.Role.Center = true
    esp.Role.Outline = true
    esp.Role.ZIndex = 2
    
    -- ÐÐ°ÑÑ‚Ñ€Ð¾Ð¹ÐºÐ° Weapon
    esp.Weapon.Size = 11
    esp.Weapon.Center = true
    esp.Weapon.Outline = true
    esp.Weapon.ZIndex = 2
    
    CreateSkeleton(player)
end

local function GetPlayerRole(player)
    if Workspace:FindFirstChild("Lobby") then
        return "Lobby"
    elseif player:FindFirstChild("Role") then
        return player.Role.Value
    elseif player.Backpack:FindFirstChild("Gun") or (player.Character and player.Character:FindFirstChild("Gun")) then
        return "Sheriff"
    elseif player.Backpack:FindFirstChild("Knife") or (player.Character and player.Character:FindFirstChild("Knife")) then
        return "Murderer"
    else
        return "Innocent"
    end
end

local function GetPlayerWeapon(player)
    if player.Character then
        for _, tool in pairs(player.Character:GetChildren()) do
            if tool:IsA("Tool") then
                return tool.Name
            end
        end
    end
    if player.Backpack then
        for _, tool in pairs(player.Backpack:GetChildren()) do
            if tool:IsA("Tool") then
                return tool.Name
            end
        end
    end
    return "None"
end

local function UpdateESP()
    for player, esp in pairs(ESPObjects) do
        if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            local hrp = player.Character.HumanoidRootPart
            local head = player.Character:FindFirstChild("Head")
            local humanoid = player.Character:FindFirstChild("Humanoid")
            
            local pos, onScreen = Camera:WorldToViewportPoint(hrp.Position)
            local role = GetPlayerRole(player)
            local color = RoleColors[role] or RoleColors.Lobby
            
            if onScreen then
                -- Box ESP
                if CheatState.ESP.Boxes then
                    local size = Vector2.new(2000 / pos.Z, 4000 / pos.Z)
                    esp.Box.Size = size
                    esp.Box.Position = Vector2.new(pos.X - size.X / 2, pos.Y - size.Y / 2)
                    esp.Box.Color = color
                    esp.Box.Visible = CheatState.ESP.Enabled
                else
                    esp.Box.Visible = false
                end
                
                -- Name ESP
                if CheatState.ESP.Names and head then
                    esp.Name.Text = player.Name
                    esp.Name.Position = Vector2.new(pos.X, pos.Y - size.Y / 2 - 20)
                    esp.Name.Color = color
                    esp.Name.Visible = CheatState.ESP.Enabled
                else
                    esp.Name.Visible = false
                end
                
                -- Role ESP
                if CheatState.ESP.Role then
                    esp.Role.Text = "[" .. role .. "]"
                    esp.Role.Position = Vector2.new(pos.X, pos.Y - size.Y / 2 - 5)
                    esp.Role.Color = color
                    esp.Role.Visible = CheatState.ESP.Enabled
                else
                    esp.Role.Visible = false
                end
                
                -- Weapon ESP
                if CheatState.ESP.Weapon then
                    local weapon = GetPlayerWeapon(player)
                    esp.Weapon.Text = "âš”ï¸ " .. weapon
                    esp.Weapon.Position = Vector2.new(pos.X, pos.Y + size.Y / 2 + 25)
                    esp.Weapon.Color = color
                    esp.Weapon.Visible = CheatState.ESP.Enabled
                else
                    esp.Weapon.Visible = false
                end
                
                -- Tracer ESP
                if CheatState.ESP.Tracers then
                    esp.Tracer.From = Vector2.new(Camera.ViewportSize.X / 2, Camera.ViewportSize.Y)
                    esp.Tracer.To = Vector2.new(pos.X, pos.Y)
                    esp.Tracer.Color = color
                    esp.Tracer.Visible = CheatState.ESP.Enabled
                else
                    esp.Tracer.Visible = false
                end
                
                -- Health Bar ESP
                if CheatState.ESP.HealthBars and humanoid then
                    local health = humanoid.Health / humanoid.MaxHealth
                    local barSize = 40
                    local barY = pos.Y + size.Y / 2 + 5
                    
                    esp.HealthBar.From = Vector2.new(pos.X - barSize / 2, barY)
                    esp.HealthBar.To = Vector2.new(pos.X - barSize / 2 + barSize * health, barY)
                    esp.HealthBar.Color = Color3.fromRGB(255 * (1 - health), 255 * health, 0)
                    esp.HealthBar.Visible = CheatState.ESP.Enabled
                    
                    esp.HealthText.Text = math.floor(humanoid.Health) .. "/" .. math.floor(humanoid.MaxHealth)
                    esp.HealthText.Position = Vector2.new(pos.X, barY + 8)
                    esp.HealthText.Color = Color3.fromRGB(255, 255, 255)
                    esp.HealthText.Visible = CheatState.ESP.Enabled
                else
                    esp.HealthBar.Visible = false
                    esp.HealthText.Visible = false
                end
                
                -- Distance ESP
                if CheatState.ESP.Distance and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                    local distance = math.floor((LocalPlayer.Character.HumanoidRootPart.Position - hrp.Position).Magnitude)
                    esp.Distance.Text = distance .. "m"
                    esp.Distance.Position = Vector2.new(pos.X, pos.Y + size.Y / 2 + 45)
                    esp.Distance.Color = color
                    esp.Distance.Visible = CheatState.ESP.Enabled
                else
                    esp.Distance.Visible = false
                end
                
                -- Update Skeleton
                if CheatState.ESP.Skeleton then
                    UpdateSkeleton(player)
                end
            else
                for _, drawing in pairs(esp) do
                    drawing.Visible = false
                end
                if Skeletons[player] then
                    for _, line in pairs(Skeletons[player]) do
                        line.Visible = false
                    end
                end
            end
        else
            for _, drawing in pairs(esp) do
                drawing.Visible = false
            end
            if Skeletons[player] then
                for _, line in pairs(Skeletons[player]) do
                    line.Visible = false
                end
            end
        end
    end
end

-- ðŸš€ Ð£ÐÐ˜ÐšÐÐ›Ð¬ÐÐ«Ð• Ð¤Ð£ÐÐšÐ¦Ð˜Ð˜
local function RainbowESP()
    while CheatState.Unique.RainbowESP do
        for _, role in pairs(RoleColors) do
            local hue = tick() % 5 / 5
            role = Color3.fromHSV(hue, 1, 1)
        end
        wait(0.1)
    end
end

local function PlayerMagnet()
    while CheatState.Unique.PlayerMagnet do
        for _, player in pairs(Players:GetPlayers()) do
            if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                    LocalPlayer.Character.HumanoidRootPart.CFrame = player.Character.HumanoidRootPart.CFrame
                end
            end
        end
        wait(1)
    end
end

local function ItemMagnet()
    while CheatState.Unique.ItemMagnet do
        for _, item in pairs(Workspace:GetChildren()) do
            if string.lower(item.Name):find("coin") or string.lower(item.Name):find("candy") or string.lower(item.Name):find("gun") then
                if item:IsA("Part") and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                    LocalPlayer.Character.HumanoidRootPart.CFrame = item.CFrame
                end
            end
        end
        wait(0.5)
    end
end

-- ðŸŽ® Ð¡Ð˜Ð¡Ð¢Ð•ÐœÐ ÐÐ’Ð¢Ðž-Ð¤ÐÐ ÐœÐ
local function StartPremiumFarm()
    while CheatState.AutoFarm.AutoCollect do
        local items = {}
        
        -- ÐŸÐ¾Ð¸ÑÐº Ð²ÑÐµÑ… collectible items
        for _, obj in pairs(Workspace:GetChildren()) do
            if string.lower(obj.Name):find("coin") or string.lower(obj.Name):find("candy") or string.lower(obj.Name):find("gun") then
                if obj:IsA("Part") then
                    table.insert(items, obj)
                end
            end
        end
        
        if #items > 0 and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
            local closestItem = items[1]
            local closestDistance = math.huge
            local hrp = LocalPlayer.Character.HumanoidRootPart
            
            for _, item in pairs(items) do
                local distance = (hrp.Position - item.Position).Magnitude
                if distance < closestDistance and distance < CheatState.AutoFarm.Radius then
                    closestDistance = distance
                    closestItem = item
                end
            end
            
            if closestDistance < CheatState.AutoFarm.Radius then
                hrp.CFrame = closestItem.CFrame + Vector3.new(0, 3, 0)
            end
        end
        
        wait(CheatState.AutoFarm.Speed / 1000)
    end
end

