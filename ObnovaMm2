   game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("[SYSTEM] Volt X Hub –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω!", "All")
end})

TrollTab:CreateButton({Name = "üîÑ –°–ü–ê–ú –¢–†–ï–ô–î–ê–ú–ò", Callback = function()
   for _, player in pairs(Players:GetPlayers()) do
       if player ~= LP then
           game.ReplicatedStorage:WaitForChild("TradeRequest"):FireServer(player)
           wait(0.2)
       end
   end
end})

