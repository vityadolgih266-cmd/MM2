AutoTab:CreateToggle({Name = "üî´ –ê–í–¢–û-–ü–û–î–ë–û–† –ü–£–®–ö–ò", CurrentValue = false, Flag = "AutoGun", Callback = function(v) 
   Volt.Automation.AutoGun = v 
end})

-- üòà –í–ö–õ–ê–î–ö–ê –¢–†–û–õ–õ–ò–ù–ì
