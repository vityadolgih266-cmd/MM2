-- ‚è∞ –£–ª—É—á—à–µ–Ω–Ω–∞—è —Å–∏—Å—Ç–µ–º–∞ —Ç–∞–π–º–µ—Ä–∞
local Round = {
   StartTime = tick(),
   Duration = 300,
   TimerVisible = true
}

local function GetRoundTime()
   if not Volt.Utility.RoundTimer then return "" end
   local elapsed = tick() - Round.StartTime
   local remaining = math.max(0, Round.Duration - elapsed)
   local minutes = math.floor(remaining / 60)
   local seconds = math.floor(remaining % 60)
   return string.format("‚è∞ %02d:%02d", minutes, seconds)
end

-- üõ°Ô∏è –ü—Ä–æ–¥–≤–∏–Ω—É—Ç—ã–π –ê–Ω—Ç–∏-–ê–§–ö
local function VoltAntiAFK()
   if not Volt.Utility.AntiAFK then return end
   
   local VirtualInput = game:GetService("VirtualInputManager")
   local lastPosition = LP.Character.HumanoidRootPart.Position
   
   coroutine.wrap(function()
       while Volt.Utility.AntiAFK do
           wait(45)
           -- –ü—Ä–æ–≤–µ—Ä–∫–∞ –Ω–∞ –¥–≤–∏–∂–µ–Ω–∏–µ
           if (LP.Character.HumanoidRootPart.Position - lastPosition).Magnitude < 5 then
               VirtualInput:SendKeyEvent(true, "W", false, game)
               wait(0.1)
               VirtualInput:SendKeyEvent(false, "W", false, game)
               wait(0.1)
               VirtualInput:SendKeyEvent(true, "S", false, game)
               wait(0.1)
               VirtualInput:SendKeyEvent(false, "S", false, game)
           end
           lastPosition = LP.Character.HumanoidRootPart.Position
       end
   end)()
end

-- üî• –≠–õ–ò–¢–ù–ê–Ø ESP –°–ò–°–¢–ï–ú–ê
local ESP = {}
local Chams = {}

local function CreateVoltESP(player)
   if ESP[player] then return end
   
   ESP[player] = {
       Box = Drawing.new("Quad"),
       Name = Drawing.new("Text"),
       HealthBar = Drawing.new("Line"),
       HealthText = Drawing.new("Text"),
       DistanceText = Drawing.new("Text"),
       RoleText = Drawing.new("Text"),
       WeaponText = Drawing.new("Text"),
       Tracer = Drawing.new("Line"),
       InfoBox = Drawing.new("Square"),
       Skeleton = {}
   }
   
   local e = ESP[player]
   
   -- üì¶ –ü—Ä–æ–¥–≤–∏–Ω—É—Ç—ã–π Box
   e.Box.Thickness = 2
   e.Box.Filled = false
   
   -- üë§ –ò–º—è
   e.Name.Size = 14
   e.Name.Center = true
   e.Name.Outline = true
   e.Name.Font = 2
   
   -- ‚ù§Ô∏è Health Bar
   e.HealthBar.Thickness = 3
   
   -- üî¢ Health Text
   e.HealthText.Size = 12
   e.HealthText.Center = true
   e.HealthText.Outline = true
   
   -- üìè Distance
   e.DistanceText.Size = 12
   e.DistanceText.Center = true
   e.DistanceText.Outline = true
   
   -- üéØ Role
   e.RoleText.Size = 13
   e.RoleText.Center = true
   e.RoleText.Outline = true
   e.RoleText.Font = 2
   
   -- üî´ Weapon
   e.WeaponText.Size = 11
   e.WeaponText.Center = true
   e.WeaponText.Outline = true
   
   -- üìç Tracer
   e.Tracer.Thickness = 1
   
   -- ‚ÑπÔ∏è Info Box
   e.InfoBox.Thickness = 1
   e.InfoBox.Filled = true
   e.InfoBox.Transparency = 0.3
   
   -- ü¶¥ Skeleton
   for i = 1, 8 do
       e.Skeleton[i] = Drawing.new("Line")
       e.Skeleton[i].Thickness = 1.5
   end
end

-- üé® Chams —Å–∏—Å—Ç–µ–º–∞
local function CreateChams(player)
   if Chams[player] then return end
   
   Chams[player] = {}
   if player.Character then
       for _, part in pairs(player.Character:GetChildren()) do
           if part:IsA("BasePart") and part.Name ~= "HumanoidRootPart" then
               local highlight = Instance.new("Highlight")
               highlight.Name = "VoltChams"
               highlight.FillColor = VoltColors.Murderer
               highlight.OutlineColor = Color3.new(1, 1, 1)
               highlight.FillTransparency = 0.5
               highlight.OutlineTransparency = 0
               highlight.Parent = part
               table.insert(Chams[player], highlight)
           end
       end
   end
end

local function UpdateChams()
   for player, highlights in pairs(Chams) do
       if player and player.Character then
           local role = GetRole(player)
           local color = VoltColors[role] or VoltColors.Innocent
           
           for _, highlight in pairs(highlights) do
               if highlight and highlight.Parent then
                   highlight.FillColor = color
                   highlight.Visible = Volt.ESP.Chams
               end
           end
       else
           for _, highlight in pairs(highlights) do
               highlight:Destroy()
           end
           Chams[player] = nil
       end
   end
end

-- üîç –£–ª—É—á—à–µ–Ω–Ω–∞—è —Å–∏—Å—Ç–µ–º–∞ —Ä–æ–ª–µ–π
local function GetRole(player)
   if not player.Character then return "Innocent" end
   
   local char = player.Character
   local backpack = player.Backpack
   
   if char:FindFirstChild("Knife") or (backpack and backpack:FindFirstChild("Knife")) then
       return "Murderer"
   elseif char:FindFirstChild("Gun") or (backpack and backpack:FindFirstChild("Gun")) then
       return "Sheriff"
   elseif char:FindFirstChildOfClass("Tool") then
       return "Sheriff"
   end
   
   return "Innocent"
end

-- üîÑ –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ ESP
local function UpdateVoltESP()
   for player, e in pairs(ESP) do
       if not player or not player.Character or not player.Character:FindFirstChild("HumanoidRootPart") then
           for _, d in pairs(e) do 
               if type(d) == "table" then
                   for _, line in pairs(d) do
                       if line then line.Visible = false end
                   end
               elseif d then
                   d.Visible = false
               end
           end
           continue
       end
       
       local char = player.Character
       local hrp = char.HumanoidRootPart
       local humanoid = char:FindFirstChild("Humanoid")
       
       if not humanoid then continue end
       
       local pos, onScreen = Camera:WorldToViewportPoint(hrp.Position)
       if not onScreen then
           for _, d in pairs(e) do 
               if type(d) == "table" then
                   for _, line in pairs(d) do
                       if line then line.Visible = false end
                   end
               elseif d then
                   d.Visible = false
               end
           end
           continue
       end
       
       local role = GetRole(player)
       local color = VoltColors[role] or VoltColors.Innocent
       local dist = math.floor((LP.Character.HumanoidRootPart.Position - hrp.Position).Magnitude)
       local health = humanoid.Health
       local maxHealth = humanoid.MaxHealth
       local healthPct = health / maxHealth
       
       -- üì¶ Box ESP
       if Volt.ESP.Box then
           local size = Vector2.new(35, 60)
           e.Box.PointA = Vector2.new(pos.X - size.X/2, pos.Y - size.Y/2)
           e.Box.PointB = Vector2.new(pos.X + size.X/2, pos.Y - size.Y/2)
           e.Box.PointC = Vector2.new(pos.X + size.X/2, pos.Y + size.Y/2)
           e.Box.PointD = Vector2.new(pos.X - size.X/2, pos.Y + size.Y/2)
           e.Box.Color = color
           e.Box.Visible = Volt.ESP.Enabled
       else e.Box.Visible = false end
       
       -- üë§ Name ESP
       if Volt.ESP.Name then
           e.Name.Text = player.Name
           e.Name.Position = Vector2.new(pos.X, pos.Y - 45)
           e.Name.Color = color
           e.Name.Visible = Volt.ESP.Enabled
       else e.Name.Visible = false end
       
       -- üéØ Role ESP
       if Volt.ESP.Role then
           e.RoleText.Text = "["..string.upper(role).."]"
           e.RoleText.Position = Vector2.new(pos.X, pos.Y - 60)
           e.RoleText.Color = color
           e.RoleText.Visible = Volt.ESP.Enabled
       else e.RoleText.Visible = false end
       
       -- ‚ù§Ô∏è Health Bar
       if Volt.ESP.Health then
           local barLen = 40
           local healthLen = barLen * healthPct
           
           e.HealthBar.From = Vector2.new(pos.X - 20, pos.Y + 35)
           e.HealthBar.To = Vector2.new(pos.X - 20 + healthLen, pos.Y + 35)
           e.HealthBar.Color = Color3.fromRGB(255 - (255 * healthPct), 255 * healthPct, 0)
           e.HealthBar.Visible = Volt.ESP.Enabled
           
           e.HealthText.Text = math.floor(health).."HP"
           e.HealthText.Position = Vector2.new(pos.X, pos.Y + 30)
           e.HealthText.Color = color
           e.HealthText.Visible = Volt.ESP.Enabled
       else
           e.HealthBar.Visible = false
           e.HealthText.Visible = false
       end
       
       -- üìè Distance
       if Volt.ESP.Distance then
           e.DistanceText.Text = dist.."m"
           e.DistanceText.Position = Vector2.new(pos.X, pos.Y + 55)
           e.DistanceText.Color = color
           e.DistanceText.Visible = Volt.ESP.Enabled
       else e.DistanceText.Visible = false end
   end
end

-- ‚ö° –≠–ö–°–ö–õ–Æ–ó–ò–í–ù–´–ï –§–£–ù–ö–¶–ò–ò VOLT X

-- üî™ Auto-Knife (–∞–≤—Ç–æ-–∞—Ç–∞–∫–∞ –Ω–æ–∂–æ–º)
local function AutoKnife()
   if not Volt.Combat.AutoKnife then return end
   
   for _, player in pairs(Players:GetPlayers()) do
       if player ~= LP and GetRole(player) == "Murderer" and player.Character then
           local dist = (LP.Character.HumanoidRootPart.Position - player.Character.HumanoidRootPart.Position).Magnitude
           if dist < 15 then
               -- –°–∏–º—É–ª—è—Ü–∏—è –∞—Ç–∞–∫–∏ –Ω–æ–∂–æ–º
               game:GetService("VirtualInputManager"):SendKeyEvent(true, "E", false, game)
               wait(0.1)
               game:GetService("VirtualInputManager"):SendKeyEvent(false, "E", false, game)
           end
       end
   end
end

-- üíÄ Kill All (–∞–≤—Ç–æ-—É–±–∏–π—Å—Ç–≤–æ –≤—Å–µ—Ö)
local function KillAll()
   if not Volt.Combat.KillAll then return end
   
   coroutine.wrap(function()
       while Volt.Combat.KillAll do
           for _, player in pairs(Players:GetPlayers()) do
               if player ~= LP and player.Character and player.Character:FindFirstChild("Humanoid") then
                   local dist = (LP.Character.HumanoidRootPart.Position - player.Character.HumanoidRootPart.Position).Magnitude
                   if dist < 20 then
                       -- –¢–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è –∫ –∏–≥—Ä–æ–∫—É –∏ "–∞—Ç–∞–∫–∞"
                       LP.Character.HumanoidRootPart.CFrame = player.Character.HumanoidRootPart.CFrame
                       wait(0.2)
                   end
               end
           end
           wait(1)
       end
   end)()
end

-- üç¨ Auto Candy Farm (—Ñ–∞—Ä–º –∫–æ–Ω—Ñ–µ—Ç)
local function AutoCandy()
   if not Volt.Automation.AutoCandy then return end
   
   local candies = {}
   for _, obj in pairs(workspace:GetDescendants()) do
       if obj.Name:lower():find("candy") and obj:IsA("Part") then
           table.insert(candies, obj)
       end
   end
   
   if #candies > 0 then
       local closest = candies[1]
       local closestDist = (LP.Character.HumanoidRootPart.Position - closest.Position).Magnitude
       
       for i = 2, #candies do
           local dist = (LP.Character.HumanoidRootPart.Position - candies[i].Position).Magnitude
           if dist < closestDist then
               closest = candies[i]
               closestDist = dist
           end
       end
       
       LP.Character.HumanoidRootPart.CFrame = closest.CFrame + Vector3.new(0, 3, 0)
   end
end

-- üéØ Aimbot —Å–∏—Å—Ç–µ–º–∞
local function VoltAimbot()
   if not Volt.Combat.Aimbot then return end
   
   local closestPlayer = nil
   local closestDistance = Volt.Combat.FOV
   
   for _, player in pairs(Players:GetPlayers()) do
       if player ~= LP and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
           local screenPoint = Camera:WorldToViewportPoint(player.Character.HumanoidRootPart.Position)
           local magnitude = (Vector2.new(screenPoint.X, screenPoint.Y) - Vector2.new(Camera.ViewportSize.X/2, Camera.ViewportSize.Y/2)).Magnitude
           
           if magnitude < closestDistance then
               closestDistance = magnitude
               closestPlayer = player
           end
       end
   end
   
   if closestPlayer then
       local targetPos = closestPlayer.Character.HumanoidRootPart.Position
       Camera.CFrame = CFrame.lookAt(Camera.CFrame.Position, targetPos)
   end
end

-- üöÄ –î–≤–∏–∂–µ–Ω–∏–µ
local function VoltFly()
   if not Volt.Movement.Fly then 
       if LP.Character and LP.Character.HumanoidRootPart:FindFirstChild("VoltFly") then
           LP.Character.HumanoidRootPart.VoltFly:Destroy()
       end
       return 
   end
   
   local bv = Instance.new("BodyVelocity")
   bv.Name = "VoltFly"
   bv.Velocity = Vector3.new(0, Volt.Movement.FlySpeed, 0)
   bv.MaxForce = Vector3.new(0, 9e9, 0)
   bv.Parent = LP.Character.HumanoidRootPart
end

local function VoltSpeed()
   if LP.Character and LP.Character:FindFirstChild("Humanoid") then
       LP.Character.Humanoid.WalkSpeed = Volt.Movement.Speed and Volt.Movement.SpeedValue or 16
   end
end

