MainTab:CreateSection("‚ö° –ë—ã—Å—Ç—Ä—ã–π —Å—Ç–∞—Ä—Ç")
MainTab:CreateToggle({
    Name = "üéØ –í–∫–ª—é—á–∏—Ç—å –≤—Å–µ –æ—Å–Ω–æ–≤–Ω—ã–µ —Ñ—É–Ω–∫—Ü–∏–∏",
    CurrentValue = false,
    Flag = "QuickStart",
    Callback = function(Value)
        if Value then
            CheatState.ESP.Enabled = true
            CheatState.AutoFarm.Coins = true
            CheatState.Movement.Speed = true
            StartCoinFarm()
            ToggleSpeed(true)
        else
            CheatState.ESP.Enabled = false
            CheatState.AutoFarm.Coins = false
            CheatState.Movement.Speed = false
            ToggleSpeed(false)
        end
    end
})

