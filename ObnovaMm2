    Callback = function()
        Rayfield:Destroy()
        -- –û—á–∏—Å—Ç–∫–∞ –≤—Å–µ—Ö ESP –æ–±—ä–µ–∫—Ç–æ–≤
        for player, esp in pairs(ESPObjects) do
            for _, drawing in pairs(esp) do
                drawing:Remove()
