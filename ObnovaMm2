-- üî• DEATH BALL GOD MODE CHEAT - FIXED VERSION
-- üéØ –°–ê–ú–´–ô –ú–û–©–ù–´–ô –ß–ò–¢ –î–õ–Ø DEATH BALL

-- –ê–õ–¨–¢–ï–†–ù–ê–¢–ò–í–ù–ê–Ø –ë–ò–ë–õ–ò–û–¢–ï–ö–ê UI (–ì–ê–†–ê–ù–¢–ò–†–û–í–ê–ù–ù–û –†–ê–ë–û–ß–ê–Ø)
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wall%20v3"))()

-- üéÆ –û–°–ù–û–í–ù–´–ï –ù–ê–°–¢–†–û–ô–ö–ò
getgenv().GodMode = {
    AutoParry = false,
    AutoAttack = false, 
    PlayerESP = false,
    BallESP = false,
    Tracers = false,
    Boxes = false,
    Speed = false,
    WalkSpeed = 120,
    Fly = false,
    FlySpeed = 80,
    NoClip = false,
    GodMode = false,
    ParryDistance = 25,
    AttackSpeed = 0.1
}

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local player = Players.LocalPlayer

-- –°–û–ó–î–ê–ï–ú –ò–ù–¢–ï–†–§–ï–ô–° –ö–û–¢–û–†–´–ô –¢–û–ß–ù–û –ë–£–î–ï–¢ –í–ò–î–ï–ù
local Window = Library:CreateWindow("üíÄ DEATH BALL GOD MODE")

-- üéØ –ê–í–¢–û–ú–ê–¢–ò–ß–ï–°–ö–û–ï –û–ë–ù–ê–†–£–ñ–ï–ù–ò–ï –ò–ì–†–´
local function DetectGame()
    local gameData = {
        Balls = {},
        Remotes = {},
        Tools = {}
    }
    
    -- –ü–æ–∏—Å–∫ –º—è—á–µ–π
    for _, obj in pairs(workspace:GetDescendants()) do
        if obj:IsA("BasePart") and (string.find(obj.Name:lower(), "ball") or obj.Shape == Enum.PartType.Ball) then
            table.insert(gameData.Balls, obj)
        end
    end
    
    -- –ü–æ–∏—Å–∫ RemoteEvents
    for _, obj in pairs(game:GetDescendants()) do
        if obj:IsA("RemoteEvent") then
            local name = obj.Name:lower()
            if string.find(name, "throw") or string.find(name, "shoot") then
                gameData.Remotes.Throw = obj
            elseif string.find(name, "parry") or string.find(name, "block") then
                gameData.Remotes.Parry = obj
            end
        end
    end
    
    return gameData
end

local Game = DetectGame()

-- üéÆ –û–°–ù–û–í–ù–´–ï –ü–ï–†–ï–ú–ï–ù–ù–´–ï
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")
local root = character:WaitForChild("HumanoidRootPart")

-- ‚ö° –°–ò–°–¢–ï–ú–ê –ê–í–¢–û–ü–ê–†–ò–†–û–í–ê–ù–ò–Ø
local lastParry = 0
RunService.Heartbeat:Connect(function()
    if getgenv().GodMode.AutoParry then
        for _, ball in pairs(Game.Balls) do
            if ball and ball.Parent and ball.AssemblyLinearVelocity then
                local distance = (ball.Position - root.Position).Magnitude
                local ballVel = ball.AssemblyLinearVelocity
                
                if distance < getgenv().GodMode.ParryDistance and ballVel.Magnitude > 10 and tick() - lastParry > 0.2 then
                    -- –ü–∞—Ä–∏—Ä–æ–≤–∞–Ω–∏–µ
                    if Game.Remotes.Parry then
                        Game.Remotes.Parry:FireServer()
                    end
                    lastParry = tick()
                    break
                end
            end
        end
    end
end)

-- üéØ –°–ò–°–¢–ï–ú–ê –ê–í–¢–û-–ê–¢–ê–ö–ò
local lastAttack = 0
RunService.Heartbeat:Connect(function()
    if getgenv().GodMode.AutoAttack and tick() - lastAttack > getgenv().GodMode.AttackSpeed then
        for _, plr in pairs(Players:GetPlayers()) do
            if plr ~= player and plr.Character then
                local target = plr.Character
                if target:FindFirstChild("HumanoidRootPart") then
                    -- –ê—Ç–∞–∫–∞
                    if Game.Remotes.Throw then
                        Game.Remotes.Throw:FireServer()
                    end
                    break
                end
            end
        end
        lastAttack = tick()
    end
end)

-- üëÅÔ∏è ESP –°–ò–°–¢–ï–ú–ê
local ESPFolder = Instance.new("Folder")
ESPFolder.Name = "DeathBallESP"
ESPFolder.Parent = workspace

RunService.Heartbeat:Connect(function()
    -- –û—á–∏—Å—Ç–∫–∞ ESP
    for _, obj in pairs(ESPFolder:GetChildren()) do
        obj:Destroy()
    end
    
    -- Player ESP
    if getgenv().GodMode.PlayerESP then
        for _, plr in pairs(Players:GetPlayers()) do
            if plr ~= player and plr.Character then
                local char = plr.Character
                local rootPart = char:FindFirstChild("HumanoidRootPart")
                
                if rootPart then
                    -- –ë–æ–∫—Å
                    if getgenv().GodMode.Boxes then
                        local box = Instance.new("BoxHandleAdornment")
                        box.Adornee = rootPart
                        box.Size = Vector3.new(4, 6, 2)
                        box.Color3 = Color3.new(1, 0, 0)
                        box.Transparency = 0.3
                        box.AlwaysOnTop = true
                        box.Parent = ESPFolder
                    end
                    
                    -- –¢—Ä–µ–π—Å–µ—Ä
                    if getgenv().GodMode.Tracers then
                        local beam = Instance.new("Beam")
                        beam.Attachment0 = Instance.new("Attachment")
                        beam.Attachment0.Parent = root
                        beam.Attachment1 = Instance.new("Attachment") 
                        beam.Attachment1.Parent = rootPart
                        beam.Color = ColorSequence.new(Color3.new(1, 0, 0))
                        beam.Width0 = 0.2
                        beam.Width1 = 0.2
                        beam.Parent = ESPFolder
                    end
                end
            end
        end
    end
    
    -- Ball ESP
    if getgenv().GodMode.BallESP then
        for _, ball in pairs(Game.Balls) do
            if ball and ball.Parent then
                local highlight = Instance.new("Highlight")
                highlight.Adornee = ball
                highlight.FillColor = Color3.new(1, 1, 0)
                highlight.OutlineColor = Color3.new(1, 0, 0)
                highlight.FillTransparency = 0.2
                highlight.Parent = ESPFolder
            end
        end
    end
end)

-- üöÄ –°–ò–°–¢–ï–ú–ê –ü–ï–†–ï–î–í–ò–ñ–ï–ù–ò–Ø
RunService.Heartbeat:Connect(function()
    -- Speed
    if getgenv().GodMode.Speed and humanoid then
        humanoid.WalkSpeed = getgenv().GodMode.WalkSpeed
    elseif humanoid then
        humanoid.WalkSpeed = 16
    end
    
    -- NoClip
    if getgenv().GodMode.NoClip and character then
        for _, part in pairs(character:GetDescendants()) do
            if part:IsA("BasePart") then
                part.CanCollide = false
            end
        end
    end
    
    -- God Mode
    if getgenv().GodMode.GodMode and humanoid then
        humanoid.MaxHealth = math.huge
        humanoid.Health = math.huge
    end
end)

-- üéÆ –°–û–ó–î–ê–ï–ú –ò–ù–¢–ï–†–§–ï–ô–°
local CombatTab = Window:CreateFolder("‚öîÔ∏è COMBAT")
local VisualTab = Window:CreateFolder("üëÅÔ∏è VISUALS") 
local MovementTab = Window:CreateFolder("üöÄ MOVEMENT")

-- ‚öîÔ∏è COMBAT
CombatTab:Toggle("üõ°Ô∏è AUTO PARRY", function(state)
    getgenv().GodMode.AutoParry = state
end)

CombatTab:Toggle("üí• AUTO ATTACK", function(state)
    getgenv().GodMode.AutoAttack = state
end)

CombatTab:Slider("PARRY DISTANCE", {min = 10, max = 50}, function(value)
    getgenv().GodMode.ParryDistance = value
end)

CombatTab:Slider("ATTACK SPEED", {min = 0.05, max = 1}, function(value)
    getgenv().GodMode.AttackSpeed = value
end)

-- üëÅÔ∏è VISUALS
VisualTab:Toggle("üëÅÔ∏è PLAYER ESP", function(state)
    getgenv().GodMode.PlayerESP = state
end)

VisualTab:Toggle("üéØ BALL ESP", function(state)
    getgenv().GodMode.BallESP = state
end)

VisualTab:Toggle("üìè TRACERS", function(state)
    getgenv().GodMode.Tracers = state
end)

VisualTab:Toggle("üü¶ BOX ESP", function(state)
    getgenv().GodMode.Boxes = state
end)

-- üöÄ MOVEMENT
MovementTab:Toggle("üöÄ SPEED HACK", function(state)
    getgenv().GodMode.Speed = state
end)

MovementTab:Slider("WALK SPEED", {min = 16, max = 500}, function(value)
    getgenv().GodMode.WalkSpeed = value
end)

MovementTab:Toggle("üëª NO CLIP", function(state)
    getgenv().GodMode.NoClip = state
end)

MovementTab:Toggle("üõ°Ô∏è GOD MODE", function(state)
    getgenv().GodMode.GodMode = state
end)

-- üîß UTILITIES
local UtilTab = Window:CreateFolder("üîß UTILITIES")

UtilTab:Button("üîÑ –û–ë–ù–û–í–ò–¢–¨ –î–ê–ù–ù–´–ï", function()
    Game = DetectGame()
end)

UtilTab:Button("üíÄ –£–î–ê–õ–ò–¢–¨ –ì–£–ô", function()
    Library:Destroy()
    ESPFolder:Destroy()
end)

print("‚úÖ DEATH BALL GOD MODE –ê–ö–¢–ò–í–ò–†–û–í–ê–ù!")
print("üî• –ò–Ω—Ç–µ—Ä—Ñ–µ–π—Å –¥–æ–ª–∂–µ–Ω –±—ã—Ç—å –≤–∏–¥–µ–Ω —Å–µ–π—á–∞—Å!")
