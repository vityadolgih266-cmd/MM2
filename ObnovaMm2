local TweenService = game:GetService("TweenService")
local LocalPlayer = Players.LocalPlayer
local Camera = Workspace.CurrentCamera

-- Ð¡Ð¸ÑÑ‚ÐµÐ¼Ð° ÑÐ¾ÑÑ‚Ð¾ÑÐ½Ð¸Ð¹
local CheatState = {
    ESP = {
        Enabled = false,
        Players = true,
        Boxes = true,
        Names = true,
        Tracers = false,
        HealthBars = false,
        Distance = false,
