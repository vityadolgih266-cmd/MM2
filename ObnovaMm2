-- –ì–õ–û–ë–ê–õ–¨–ù–´–ï –ù–ê–°–¢–†–û–ô–ö–ò
getgenv().DeathBallHack = {
    -- –ë–æ–µ–≤—ã–µ —Ñ—É–Ω–∫—Ü–∏–∏
    AutoParry = false,
    AutoSpam = false,
    AutoFarm = false,
    AutoAbility = false,
    InstantKill = false,
    
    -- ESP —Ñ—É–Ω–∫—Ü–∏–∏
    PlayerESP = false,
    BallESP = false,
    ESPLines = false,
    ESPBoxes = false,
    ESPDistance = false,
    ESPNames = false,
    
    -- –í–∏–∑—É–∞–ª—å–Ω—ã–µ —É–ª—É—á—à–µ–Ω–∏—è
    Tracers = false,
    ShowDistance = false,
    ShowHealth = false,
    
    -- –ü–µ—Ä–µ–¥–≤–∏–∂–µ–Ω–∏–µ
    SpeedHack = false,
    WalkSpeed = 100,
    InfiniteJump = false,
    NoClip = false,
    Fly = false,
    FlySpeed = 50,
    
    -- –ù–∞—Å—Ç—Ä–æ–π–∫–∏
    ParryDistance = 20,
    PingCompensation = 0.1,
    ESPRefresh = 0.1,
    
    -- –ê–≤—Ç–æ–º–∞—Ç–∏–∑–∞—Ü–∏—è
    AutoClaimQuest = false,
    AutoRejoin = false
}

local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")
local root = character:WaitForChild("HumanoidRootPart")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local RunService = game:GetService("RunService")
local Players = game:GetService("Players")

-- –§–£–ù–ö–¶–ò–ò –ü–û–ò–°–ö–ê –û–ë–™–ï–ö–¢–û–í
local function getDeathBall()
    local balls = {}
    for _, obj in ipairs(workspace:GetDescendants()) do
        if obj:IsA("BasePart") and (string.find(obj.Name:lower(), "ball") or 
           obj.Name == "DeathBall" or obj.Shape == Enum.PartType.Ball) then
            table.insert(balls, obj)
        end
    end
    return balls
end

local function getAllPlayers()
    local players = {}
    for _, plr in ipairs(Players:GetPlayers()) do
        if plr ~= player and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
            table.insert(players, plr.Character)
        end
    end
    return players
end

-- –ü–û–ò–°–ö REMOTE EVENTS –î–õ–Ø DEATH BALL
local function findDeathBallRemotes()
    local remotes = {}
    local remoteNames = {"Throw", "Shoot", "Fire", "Attack", "Parry", "Block", "Deflect", "Ability", "Skill", "Quest", "Claim"}
    
    for _, name in ipairs(remoteNames) do
        for _, obj in ipairs(ReplicatedStorage:GetDescendants()) do
            if obj:IsA("RemoteEvent") and string.find(obj.Name:lower(), name:lower()) then
                remotes[name] = obj
            end
        end
    end
    return remotes
end

local deathBallRemotes = findDeathBallRemotes()

-- ULTIMATE AUTO PARRY –î–õ–Ø DEATH BALL
local lastParryTime = 0
spawn(function()
    RunService.Heartbeat:Connect(function()
        if getgenv().DeathBallHack.AutoParry then
            local balls = getDeathBall()
            
            for _, ball in ipairs(balls) do
                if ball and ball.Parent and ball.AssemblyLinearVelocity then
                    local ballPos = ball.Position
                    local rootPos = root.Position
                    local distance = (ballPos - rootPos).Magnitude
                    local ballVel = ball.AssemblyLinearVelocity
                    local ballSpeed = ballVel.Magnitude
                    
                    -- –ê–ì–†–ï–°–°–ò–í–ù–û–ï –ü–ê–†–ò–†–û–í–ê–ù–ò–ï –î–õ–Ø DEATH BALL
                    local isDangerous = ballSpeed > 10
                    local isClose = distance < getgenv().DeathBallHack.ParryDistance
                    local isComingAtMe = ballVel:Dot((rootPos - ballPos).Unit) > 0.3
                    
                    if isDangerous and isClose and isComingAtMe and tick() - lastParryTime > 0.15 then
                        
                        -- –ú–£–õ–¨–¢–ò-–ü–ê–†–ò–†–û–í–ê–ù–ò–ï –í–°–ï–ú–ò –°–ü–û–°–û–ë–ê–ú–ò
                        for i = 1, 5 do
                            -- –ß–µ—Ä–µ–∑ RemoteEvents
                            if deathBallRemotes["Parry"] then
                                deathBallRemotes["Parry"]:FireServer()
                            end
                            if deathBallRemotes["Block"] then
                                deathBallRemotes["Block"]:FireServer()
                            end
                            
                            -- –ß–µ—Ä–µ–∑ –∏–Ω—Å—Ç—Ä—É–º–µ–Ω—Ç—ã
                            for _, tool in ipairs(character:GetChildren()) do
                                if tool:IsA("Tool") then
                                    pcall(function()
                                        tool:Activate()
                                        task.wait(0.01)
                                        tool:Deactivate()
                                    end)
                                end
                            end
                            
                            -- –≠–º—É–ª—è—Ü–∏—è –∫–ª–∞–≤–∏—à
                            pcall(function()
                                local virtualInput = game:GetService("VirtualInputManager")
                                virtualInput:SendKeyEvent(true, Enum.KeyCode.E, false, game)
                                virtualInput:SendKeyEvent(true, Enum.KeyCode.Q, false, game)
                                virtualInput:SendKeyEvent(true, Enum.KeyCode.F, false, game)
                                task.wait(0.01)
                                virtualInput:SendKeyEvent(false, Enum.KeyCode.E, false, game)
                                virtualInput:SendKeyEvent(false, Enum.KeyCode.Q, false, game)
                                virtualInput:SendKeyEvent(false, Enum.KeyCode.F, false, game)
                            end)
                            
                            task.wait(0.03)
                        end
                        
                        lastParryTime = tick()
                        break
                    end
                end
            end
        end
    end)
end)

-- HYPER AUTO SPAM –î–õ–Ø DEATH BALL
spawn(function()
    while true do
        task.wait(0.06) -- –°–£–ü–ï–† –ë–´–°–¢–†–´–ô –°–ü–ê–ú
        if getgenv().DeathBallHack.AutoSpam then
            -- –°–ü–ê–ú –í–°–ï–ú–ò REMOTE EVENTS –û–î–ù–û–í–†–ï–ú–ï–ù–ù–û
            for name, remote in pairs(deathBallRemotes) do
                if name ~= "Quest" and name ~= "Claim" then
                    for i = 1, 2 do
                        pcall(function() remote:FireServer() end)
                    end
                end
            end
            
            -- –°–ü–ê–ú –ò–ù–°–¢–†–£–ú–ï–ù–¢–ê–ú–ò
            for _, tool in ipairs(character:GetChildren()) do
                if tool:IsA("Tool") then
                    pcall(function()
                        tool:Activate()
                        task.wait(0.01)
                        tool:Deactivate()
                    end)
                end
            end
        end
    end
end)

-- COMPLETE ESP SYSTEM –î–õ–Ø DEATH BALL
local espFolder = Instance.new("Folder")
espFolder.Name = "DeathBallESP"
espFolder.Parent = workspace

local playerESP = {}
local ballESP = {}
local tracerConnections = {}

-- ESP –î–õ–Ø –ò–ì–†–û–ö–û–í
spawn(function()
    RunService.Heartbeat:Connect(function()
        -- –û—á–∏—Å—Ç–∫–∞ —Å—Ç–∞—Ä—ã—Ö ESP
        for _, esp in pairs(playerESP) do
            if esp.box then esp.box:Destroy() end
            if esp.name then esp.name:Destroy() end
            if esp.distance then esp.distance:Destroy() end
            if esp.health then esp.health:Destroy() end
            if esp.tracer then esp.tracer:Destroy() end
        end
        playerESP = {}
        
        if getgenv().DeathBallHack.PlayerESP then
            local players = getAllPlayers()
            
            for _, enemy in ipairs(players) do
                if enemy and enemy.Parent and enemy:FindFirstChild("HumanoidRootPart") then
                    local enemyRoot = enemy.HumanoidRootPart
                    local enemyHumanoid = enemy:FindFirstChild("Humanoid")
                    
                    -- BOX ESP
                    if getgenv().DeathBallHack.ESPBoxes then
                        local box = Instance.new("BoxHandleAdornment")
                        box.Adornee = enemyRoot
                        box.Size = enemyRoot.Size + Vector3.new(0.5, 0.5, 0.5)
                        box.Color3 = Color3.fromRGB(255, 0, 0)
                        box.Transparency = 0.3
                        box.AlwaysOnTop = true
                        box.ZIndex = 10
                        box.Parent = espFolder
                        playerESP[enemy] = playerESP[enemy] or {}
                        playerESP[enemy].box = box
                    end
                    
                    -- NAME ESP
                    if getgenv().DeathBallHack.ESPNames then
                        local billboard = Instance.new("BillboardGui")
                        billboard.Adornee = enemyRoot
                        billboard.Size = UDim2.new(0, 200, 0, 50)
                        billboard.StudsOffset = Vector3.new(0, 3, 0)
                        billboard.AlwaysOnTop = true
                        billboard.MaxDistance = 1000
                        
                        local nameLabel = Instance.new("TextLabel")
                        nameLabel.Size = UDim2.new(1, 0, 0.5, 0)
                        nameLabel.BackgroundTransparency = 1
                        nameLabel.Text = enemy.Parent.Name
                        nameLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
                        nameLabel.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
                        nameLabel.TextStrokeTransparency = 0
                        nameLabel.TextScaled = true
                        nameLabel.Font = Enum.Font.GothamBold
                        nameLabel.Parent = billboard
                        
                        billboard.Parent = espFolder
                        playerESP[enemy] = playerESP[enemy] or {}
                        playerESP[enemy].name = billboard
                    end
                    
                    -- DISTANCE ESP
                    if getgenv().DeathBallHack.ESPDistance then
                        local distance = (enemyRoot.Position - root.Position).Magnitude
                        
                        local billboard = Instance.new("BillboardGui")
                        billboard.Adornee = enemyRoot
                        billboard.Size = UDim2.new(0, 150, 0, 30)
                        billboard.StudsOffset = Vector3.new(0, 2.5, 0)
                        billboard.AlwaysOnTop = true
                        
                        local distanceLabel = Instance.new("TextLabel")
                        distanceLabel.Size = UDim2.new(1, 0, 1, 0)
                        distanceLabel.BackgroundTransparency = 1
                        distanceLabel.Text = math.floor(distance) .. " studs"
                        distanceLabel.TextColor3 = Color3.fromRGB(0, 255, 255)
                        distanceLabel.TextStrokeTransparency = 0
                        distanceLabel.TextScaled = true
                        distanceLabel.Parent = billboard
                        
                        billboard.Parent = espFolder
                        playerESP[enemy] = playerESP[enemy] or {}
                        playerESP[enemy].distance = billboard
                    end
                    
                    -- HEALTH ESP
                    if getgenv().DeathBallHack.ShowHealth and enemyHumanoid then
                        local billboard = Instance.new("BillboardGui")
                        billboard.Adornee = enemyRoot
                        billboard.Size = UDim2.new(0, 120, 0, 25)
                        billboard.StudsOffset = Vector3.new(0, 2, 0)
                        billboard.AlwaysOnTop = true
                        
                        local healthLabel = Instance.new("TextLabel")
                        healthLabel.Size = UDim2.new(1, 0, 1, 0)
                        healthLabel.BackgroundTransparency = 1
                        healthLabel.Text = "HP: " .. math.floor(enemyHumanoid.Health)
                        healthLabel.TextColor3 = Color3.fromRGB(0, 255, 0)
                        healthLabel.TextStrokeTransparency = 0
                        healthLabel.TextScaled = true
                        healthLabel.Parent = billboard
                        
                        billboard.Parent = espFolder
                        playerESP[enemy] = playerESP[enemy] or {}
                        playerESP[enemy].health = billboard
                    end
                    
                    -- TRACER LINES
                    if getgenv().DeathBallHack.ESPLines then
                        local tracer = Instance.new("Beam")
                        tracer.Attachment0 = Instance.new("Attachment")
                        tracer.Attachment0.Parent = root
                        tracer.Attachment1 = Instance.new("Attachment")
                        tracer.Attachment1.Parent = enemyRoot
                        
                        -- –¶–≤–µ—Ç —Ç—Ä–∞—Å—Å–µ—Ä–∞ –≤ –∑–∞–≤–∏—Å–∏–º–æ—Å—Ç–∏ –æ—Ç —Ä–∞—Å—Å—Ç–æ—è–Ω–∏—è
                        local dist = (enemyRoot.Position - root.Position).Magnitude
                        if dist < 20 then
                            tracer.Color = ColorSequence.new(Color3.fromRGB(255, 0, 0)) -- –ö—Ä–∞—Å–Ω—ã–π (–±–ª–∏–∑–∫–æ)
                        elseif dist < 50 then
                            tracer.Color = ColorSequence.new(Color3.fromRGB(255, 255, 0)) -- –ñ–µ–ª—Ç—ã–π (—Å—Ä–µ–¥–Ω–µ)
                        else
                            tracer.Color = ColorSequence.new(Color3.fromRGB(0, 255, 0)) -- –ó–µ–ª–µ–Ω—ã–π (–¥–∞–ª–µ–∫–æ)
                        end
                        
                        tracer.Width0 = 0.2
                        tracer.Width1 = 0.2
                        tracer.FaceCamera = true
                        tracer.Parent = espFolder
                        
                        playerESP[enemy] = playerESP[enemy] or {}
                        playerESP[enemy].tracer = tracer
                    end
                end
            end
        end
    end)
end)

-- ESP –î–õ–Ø –ú–Ø–ß–ê
spawn(function()
    RunService.Heartbeat:Connect(function()
        -- –û—á–∏—Å—Ç–∫–∞ ESP –º—è—á–∞
        for _, esp in pairs(ballESP) do
            if esp.highlight then esp.highlight:Destroy() end
            if esp.billboard then esp.billboard:Destroy() end
            if esp.tracer then esp.tracer:Destroy() end
        end
        ballESP = {}
        
        if getgenv().DeathBallHack.BallESP then
            local balls = getDeathBall()
            
            for _, ball in ipairs(balls) do
                if ball and ball.Parent then
                    -- HIGHLIGHT
                    local highlight = Instance.new("Highlight")
                    highlight.Adornee = ball
                    highlight.FillColor = Color3.fromRGB(255, 255, 0) -- –Ø—Ä–∫–æ-–∂–µ–ª—Ç—ã–π
                    highlight.OutlineColor = Color3.fromRGB(255, 0, 0) -- –ö—Ä–∞—Å–Ω–∞—è –æ–±–≤–æ–¥–∫–∞
                    highlight.FillTransparency = 0.1
                    highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
                    highlight.Parent = espFolder
                    
                    -- BILLBOARD –° –ò–ù–§–û–†–ú–ê–¶–ò–ï–ô
                    local billboard = Instance.new("BillboardGui")
                    billboard.Adornee = ball
                    billboard.Size = UDim2.new(0, 200, 0, 80)
                    billboard.StudsOffset = Vector3.new(0, 4, 0)
                    billboard.AlwaysOnTop = true
                    billboard.MaxDistance = 2000
                    
                    local distanceText = math.floor((ball.Position - root.Position).Magnitude)
                    local speedText = math.floor(ball.AssemblyLinearVelocity.Magnitude)
                    
                    local label = Instance.new("TextLabel")
                    label.Size = UDim2.new(1, 0, 1, 0)
                    label.BackgroundTransparency = 1
                    label.Text = "üéØ DEATH BALL\n" .. distanceText .. " studs\n" .. speedText .. " speed"
                    label.TextColor3 = Color3.fromRGB(255, 255, 0)
                    label.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
                    label.TextStrokeTransparency = 0
                    label.TextScaled = true
                    label.Font = Enum.Font.GothamBold
                    label.Parent = billboard
                    billboard.Parent = espFolder
                    
                    -- TRACER –ö –ú–Ø–ß–£
                    if getgenv().DeathBallHack.Tracers then
                        local tracer = Instance.new("Beam")
                        tracer.Attachment0 = Instance.new("Attachment")
                        tracer.Attachment0.Parent = root
                        tracer.Attachment1 = Instance.new("Attachment")
                        tracer.Attachment1.Parent = ball
                        tracer.Color = ColorSequence.new(Color3.fromRGB(255, 255, 0))
                        tracer.Width0 = 0.3
                        tracer.Width1 = 0.3
                        tracer.FaceCamera = true
                        tracer.Parent = espFolder
                        
                        ballESP[ball] = {highlight = highlight, billboard = billboard, tracer = tracer}
                    else
                        ballESP[ball] = {highlight = highlight, billboard = billboard}
                    end
                end
            end
        end
    end)
end)

-- –î–û–ü–û–õ–ù–ò–¢–ï–õ–¨–ù–´–ï –§–£–ù–ö–¶–ò–ò –î–õ–Ø DEATH BALL
-- Auto Farm
spawn(function()
    while true do
        task.wait(0.2)
        if getgenv().DeathBallHack.AutoFarm then
            local players = getAllPlayers()
            if #players > 0 then
                local target = players[1]
                if target:FindFirstChild("HumanoidRootPart") then
                    -- –¢–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è –∫ —Ü–µ–ª–∏
                    root.CFrame = target.HumanoidRootPart.CFrame * CFrame.new(0, 0, -5)
                    -- –ê–≤—Ç–æ-–∞—Ç–∞–∫–∞
                    if deathBallRemotes["Throw"] then
                        for i = 1, 3 do
                            deathBallRemotes["Throw"]:FireServer()
                            task.wait(0.1)
                        end
                    end
                end
            end
        end
    end
end)

-- Speed Hack
spawn(function()
    RunService.Heartbeat:Connect(function()
        if getgenv().DeathBallHack.SpeedHack and humanoid then
            humanoid.WalkSpeed = getgenv().DeathBallHack.WalkSpeed
        elseif humanoid then
            humanoid.WalkSpeed = 16
        end
    end)
end)

-- Infinite Jump
game:GetService("UserInputService").JumpRequest:Connect(function()
    if getgenv().DeathBallHack.InfiniteJump and humanoid then
        humanoid:ChangeState("Jumping")
    end
end)

-- NoClip
spawn(function()
    RunService.Stepped:Connect(function()
        if getgenv().DeathBallHack.NoClip and character then
            for _, part in ipairs(character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = false
                end
            end
        end
    end)
end)

-- Fly
local flying = false
spawn(function()
    RunService.Heartbeat:Connect(function()
        if getgenv().DeathBallHack.Fly and character then
            flying = true
            humanoid.PlatformStand = true
            local vel = Instance.new("BodyVelocity")
            vel.Velocity = Vector3.new(0, 0, 0)
            vel.MaxForce = Vector3.new(40000, 40000, 40000)
            vel.Parent = root
            
            local function fly()
                local cam = workspace.CurrentCamera
                local move = {
                    Forward = false,
                    Backward = false,
                    Left = false,
                    Right = false,
                    Up = false,
                    Down = false
                }
                
                local connection
                connection = game:GetService("UserInputService").InputBegan:Connect(function(input)
                    if input.KeyCode == Enum.KeyCode.W then move.Forward = true end
                    if input.KeyCode == Enum.KeyCode.S then move.Backward = true end
                    if input.KeyCode == Enum.KeyCode.A then move.Left = true end
                    if input.KeyCode == Enum.KeyCode.D then move.Right = true end
                    if input.KeyCode == Enum.KeyCode.Space then move.Up = true end
                    if input.KeyCode == Enum.KeyCode.LeftShift then move.Down = true end
                end)
                
                game:GetService("UserInputService").InputEnded:Connect(function(input)
                    if input.KeyCode == Enum.KeyCode.W then move.Forward = false end
                    if input.KeyCode == Enum.KeyCode.S then move.Backward = false end
                    if input.KeyCode == Enum.KeyCode.A then move.Left = false end
                    if input.KeyCode == Enum.KeyCode.D then move.Right = false end
                    if input.KeyCode == Enum.KeyCode.Space then move.Up = false end
                    if input.KeyCode == Enum.KeyCode.LeftShift then move.Down = false end
                end)
                
                while flying do
                    task.wait()
                    local direction = Vector3.new(0, 0, 0)
                    if move.Forward then direction = direction + cam.CFrame.LookVector end
                    if move.Backward then direction = direction - cam.CFrame.LookVector end
                    if move.Left then direction = direction - cam.CFrame.RightVector end
                    if move.Right then direction = direction + cam.CFrame.RightVector end
                    if move.Up then direction = direction + Vector3.new(0, 1, 0) end
                    if move.Down then direction = direction + Vector3.new(0, -1, 0) end
                    
                    if direction.Magnitude > 0 then
                        direction = direction.Unit * getgenv().DeathBallHack.FlySpeed
                    end
                    
                    vel.Velocity = direction
                end
                
                connection:Disconnect()
            end
            
            fly()
        elseif flying then
            flying = false
            humanoid.PlatformStand = false
            for _, v in ipairs(root:GetChildren()) do
                if v:IsA("BodyVelocity") then
                    v:Destroy()
                end
            end
        end
    end)
end)

